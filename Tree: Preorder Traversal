def preOrder(root):
    
    print(root.info,end =" ")
    if root.left :
        preOrder(root.left)
    if root.right :
        preOrder(root.right)
        
        
        
        
        
def preOrder(root):
    while root:
        print(root.info, end=' ')
        preOrder(root.left)
        preOrder(root.right)
        break
